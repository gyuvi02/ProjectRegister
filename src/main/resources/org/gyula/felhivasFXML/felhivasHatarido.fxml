<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="700.0" stylesheets="@../foablak.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.felhivasController.felhivasHataridoController">
   <Label contentDisplay="CENTER" minHeight="-Infinity" minWidth="-Infinity" prefHeight="100.0" prefWidth="652.0" text="Azokat a felhívásokat keresi meg, amelyek beadási határideje a megadott dátum után van" textAlignment="CENTER" wrapText="true">
      <padding>
         <Insets top="-50.0" />
      </padding></Label>
   <HBox alignment="CENTER" prefHeight="94.0" prefWidth="594.0">
      <children>
         <Label text="A határidő megadása">
            <HBox.margin>
               <Insets right="20.0" />
            </HBox.margin>
         </Label>
         <DatePicker fx:id="hataridoGomb">
            <HBox.margin>
               <Insets />
            </HBox.margin></DatePicker>
      </children>
      <VBox.margin>
         <Insets bottom="30.0" />
      </VBox.margin></HBox>

    <HBox alignment="CENTER" prefHeight="87.0" prefWidth="694.0">
        <VBox.margin>
            <Insets bottom="-40.0" top="-10.0" />
        </VBox.margin>
        <Button fx:id="kereses" mnemonicParsing="false" onAction="#hataridoKereso" prefWidth="165.0" text="Keresés" HBox.hgrow="ALWAYS">
            <HBox.margin>
                <Insets right="20.0" />
            </HBox.margin>
        </Button>
        <Button fx:id="kilepesGomb" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#kilep" prefWidth="165.0" text="Mégse" textAlignment="CENTER">
            <HBox.margin>
                <Insets left="20.0" />
            </HBox.margin>
        </Button>
    </HBox>


</VBox>
