<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="700.0" prefWidth="600.0" stylesheets="@../foablak.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.felhivasController.felhivasOktatoController">
    <children>
        <ListView fx:id="oktatoNevek" prefHeight="620.0" prefWidth="600.0">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin></ListView>
        <HBox alignment="CENTER" prefHeight="70.0" prefWidth="400.0">
            <children>
                <Button fx:id="nevValasztoReszletek" mnemonicParsing="false" onAction="#felhivasValaszto" prefWidth="160.0" text="Kiválaszt" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets />
                    </HBox.margin></Button>
                <Button fx:id="kilepesGomb" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#kilep" prefWidth="160.0" text="Mégse" textAlignment="CENTER">
                    <HBox.margin>
                        <Insets left="80.0" />
                    </HBox.margin></Button>
            </children>
            <VBox.margin>
                <Insets />
            </VBox.margin>
        </HBox>
    </children>


</VBox>
