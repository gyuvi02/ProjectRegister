<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="450.0" prefWidth="560.0" stylesheets="@../foablak.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.felhivasController.felhivasPalyazatiTemaListaController">
    <ListView fx:id="kategoriaLista" onMouseClicked="#nincsValasztas" prefHeight="374.0" prefWidth="543.0">
        <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
        </VBox.margin></ListView>
    <HBox alignment="CENTER" prefHeight="70.0" prefWidth="400.0">
        <VBox.margin>
            <Insets top="-10.0" />
        </VBox.margin>
        <Button fx:id="felhivasValaszto" mnemonicParsing="false" onAction="#kategoriaValaszto" text="Kiválaszt" HBox.hgrow="ALWAYS">
            <HBox.margin>
                <Insets right="20.0" />
            </HBox.margin>
        </Button>
        <Button fx:id="kilepesGomb" alignment="CENTER_RIGHT" mnemonicParsing="false" onAction="#kilep" text="Bezár" textAlignment="CENTER">
            <HBox.margin>
                <Insets left="20.0" />
            </HBox.margin>
        </Button>
    </HBox>


</VBox>
