<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="600.0" prefWidth="900.0" stylesheets="@../foablak.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.oktatoController.oktatoTanszekiAktivitasController">
    <ListView fx:id="aktivitasLista" maxHeight="-Infinity" maxWidth="-Infinity" onMouseClicked="#nincsValasztas" prefHeight="520.0" prefWidth="900.0">
        <VBox.margin>
            <Insets left="10.0" right="10.0" top="10.0" />
        </VBox.margin></ListView>
    <HBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="50.0" prefWidth="700.0">
        <VBox.margin>
            <Insets top="10.0"/>
        </VBox.margin>
        <Button fx:id="palyazatReszletek" mnemonicParsing="false" onAction="#nevValaszto" prefHeight="40.0"
                prefWidth="160.0" text="Részletek" HBox.hgrow="ALWAYS">
            <HBox.margin>
                <Insets bottom="10.0"/>
            </HBox.margin>
        </Button>
        <Button fx:id="kilepesGomb" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#kilep"
                prefWidth="160.0" text="Mégse" textAlignment="CENTER">
            <HBox.margin>
                <Insets bottom="10.0" left="80.0"/>
            </HBox.margin>
        </Button>
    </HBox>


</VBox>
