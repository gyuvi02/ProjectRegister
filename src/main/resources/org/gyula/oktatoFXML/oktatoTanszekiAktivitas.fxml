<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="500.0" prefWidth="600.0" stylesheets="@../foablak.css"
      xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="controller.oktatoController.oktatoTanszekiAktivitasController">
    <ListView fx:id="aktivitasLista" maxWidth="-Infinity" prefHeight="452.0" prefWidth="580.0" onMouseClicked="#nincsValasztas">
        <VBox.margin>
            <Insets top="10.0" />
        </VBox.margin></ListView>
    <HBox maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="55.0" prefWidth="580">
        <children>
            <Button fx:id="palyazatReszletek" mnemonicParsing="false" onAction="#nevValaszto" text="Részletek" HBox.hgrow="ALWAYS">
                <HBox.margin>
                    <Insets right="20.0" />
                </HBox.margin></Button>
            <Button fx:id="kilepesGomb" alignment="CENTER_RIGHT" mnemonicParsing="false" onAction="#kilep" text="Bezár" textAlignment="CENTER">
                <HBox.margin>
                    <Insets left="20.0" />
                </HBox.margin></Button>
        </children>
        <VBox.margin>
            <Insets left="180.0" top="10.0" />
        </VBox.margin>
    </HBox>


</VBox>
