<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="400.0" prefWidth="500.0" stylesheets="@../foablak.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.oktatoController.oktatoReszletekController">
   <children>
      <HBox maxWidth="-Infinity" prefHeight="100.0" prefWidth="500.0" VBox.vgrow="ALWAYS">
         <children>
            <Label fx:id="reszletekFigyelmeztetes" alignment="CENTER" disable="true" nodeOrientation="LEFT_TO_RIGHT" text="Az adatok kimásolhatók, de itt nem módosíthatók!" textAlignment="CENTER" HBox.hgrow="ALWAYS">
               <font>
                  <Font size="14.0" />
               </font>
               <HBox.margin>
                  <Insets left="110.0" right="100.0" />
               </HBox.margin>
            </Label>
         </children>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
      </HBox>
      <TextArea fx:id="reszletek" prefHeight="475.0" prefWidth="393.0" stylesheets="@../foablak.css" wrapText="true">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin></TextArea>
      <BorderPane prefHeight="75.0" prefWidth="393.0">
         <center>
             <Button fx:id="kilepesGomb" alignment="CENTER_RIGHT" contentDisplay="TOP" mnemonicParsing="false" onAction="#kilep" text="Bezár" textAlignment="CENTER" HBox.hgrow="ALWAYS">
               <BorderPane.margin>
                  <Insets bottom="10.0" top="10.0" />
               </BorderPane.margin></Button>
         </center>
      </BorderPane>
   </children>
</VBox>
