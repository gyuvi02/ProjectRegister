<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="600.0" prefWidth="900.0" stylesheets="@../foablak.css" xmlns="http://javafx.com/javafx/10.0.2-internal"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.gyula.controller.oktatoController.oktatoReszletekController">
   <HBox maxWidth="500.0" prefHeight="23.0" prefWidth="500.0" VBox.vgrow="ALWAYS">
      <VBox.margin>
         <Insets top="10.0" />
      </VBox.margin>
      <Label alignment="CENTER" disable="true" maxWidth="500.0" minWidth="500.0"
             nodeOrientation="LEFT_TO_RIGHT" prefHeight="39.0" prefWidth="500.0"
             text="Az adatok kimásolhatók, de itt nem módosíthatók!" textAlignment="CENTER" HBox.hgrow="ALWAYS">
         <font>
            <Font size="20.0" />
         </font>
         <HBox.margin>
            <Insets bottom="10.0" top="10.0" />
         </HBox.margin>
      </Label>
   </HBox>
   <TextArea fx:id="reszletek" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="520.0" prefWidth="900.0" stylesheets="@../foablak.css" wrapText="true">
      <VBox.margin>
         <Insets bottom="5.0" left="10.0" right="10.0" top="10.0" />
      </VBox.margin>
   </TextArea>
   <Button fx:id="kilepesGomb" alignment="CENTER_RIGHT" contentDisplay="TOP" mnemonicParsing="false" onAction="#kilep" text="Bezár" textAlignment="CENTER" HBox.hgrow="ALWAYS">
      <VBox.margin>
         <Insets bottom="10.0" top="5.0" />
      </VBox.margin></Button>
</VBox>
