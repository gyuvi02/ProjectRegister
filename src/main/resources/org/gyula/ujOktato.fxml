<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="400.0" minWidth="500.0" prefHeight="400.0" prefWidth="500.0" stylesheets="@foablak.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.gyula.ujOktatoController">
   <bottom>
       <HBox alignment="CENTER">
           <Button alignment="CENTER_RIGHT" mnemonicParsing="false" onAction="#visszaOktatooldalra" text="Mégsem" BorderPane.alignment="CENTER">
               <font>
                   <Font name="Century Gothic" size="17.0" />
               </font>
               <opaqueInsets>
                   <Insets />
               </opaqueInsets>
               <HBox.margin>
                   <Insets right="200.0" />
               </HBox.margin></Button>
           <Button fx:id="hozzaadGomb" contentDisplay="CENTER" mnemonicParsing="false" onAction="#hozzaad" text="Oktató hozzáadása" textAlignment="CENTER">
               <font>
                   <Font name="Century Gothic" size="17.0" />
               </font>
           </Button>
           <BorderPane.margin>
               <Insets bottom="10.0" />
           </BorderPane.margin>
       </HBox>
   </bottom>
   <center>
      <GridPane BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="400.0" prefWidth="500.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="60.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="60.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="100.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Oktató neve:" GridPane.halignment="RIGHT" GridPane.valignment="CENTER">
               <GridPane.margin>
                  <Insets right="20.0" />
               </GridPane.margin></Label>
            <Label text="Tanszék:" GridPane.halignment="RIGHT" GridPane.rowIndex="1" GridPane.valignment="CENTER">
               <GridPane.margin>
                  <Insets right="20.0" />
               </GridPane.margin></Label>
            <Label text="Oktató email címe:" GridPane.halignment="RIGHT" GridPane.rowIndex="2" GridPane.valignment="CENTER">
               <GridPane.margin>
                  <Insets right="20.0" />
               </GridPane.margin></Label>
            <TextField fx:id="oNev" maxHeight="-Infinity" maxWidth="-Infinity" onKeyReleased="#uresMezo" prefWidth="300.0" GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.valignment="CENTER" />
            <TextField fx:id="oTanszek" maxWidth="-Infinity" onKeyReleased="#uresMezo" prefWidth="300.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="oEmail" maxWidth="-Infinity" onKeyReleased="#uresMezo" prefWidth="300.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         </children>
      </GridPane>
   </center>
</BorderPane>
