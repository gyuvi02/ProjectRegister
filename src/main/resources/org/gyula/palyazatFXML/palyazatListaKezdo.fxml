<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.*?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="controller.palyazatController.palyazatListaController"
      prefHeight="700.0" prefWidth="600.0" stylesheets="@../foablak.css">
    <ListView fx:id="palyazatLista" prefHeight="620.0" prefWidth="600.0">
        <VBox.margin>
            <Insets left="10.0" right="10.0" top="10.0" />
        </VBox.margin>
    </ListView>
    <HBox alignment="CENTER" prefHeight="70.0" prefWidth="400.0">
        <VBox.margin>
            <Insets top="-10.0" />
        </VBox.margin>
        <Button fx:id="kivalasztKezdo" mnemonicParsing="false" onAction="#palyazatKivalaszto" text="Kiválaszt" textAlignment="CENTER">
            <HBox.margin>
                <Insets right="20.0" top="10.0" />
            </HBox.margin>
        </Button>
        <Button fx:id="kilepesGomb" alignment="CENTER_RIGHT" mnemonicParsing="false" onMouseClicked="#kilep" text="Mégse" textAlignment="CENTER">
            <HBox.margin>
                <Insets left="20.0" top="10.0" />
            </HBox.margin>
        </Button>
    </HBox>
</VBox>
