         <ComboBox fx:id="fazisGomb" maxHeight="-Infinity" maxWidth="-Infinity" onAction="#uresMezo" prefHeight="50.0" prefWidth="312.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
