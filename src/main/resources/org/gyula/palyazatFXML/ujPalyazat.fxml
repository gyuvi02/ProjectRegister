                  <String fx:value="Beadott" />
                  <String fx:value="Elutasított" />
                  <String fx:value="Elfogadott" />
                  <String fx:value="Lezárt" />
               </FXCollections>
            </items>
            <value>
               <String fx:value="Válasszon fázist" />
            </value>
