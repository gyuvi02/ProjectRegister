<BorderPane prefHeight="600.0" prefWidth="950.0" stylesheets="@foablak.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.gyula.oktatoFooldalController">
